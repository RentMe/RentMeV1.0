//Dependencies
var restful = require('node-restful');
var mongoose = restful.mongoose;

//Schema
var offerTypes = new mongoose.Schema({
	OfferTypeID	:	int
  OfferType	:	varchar
  CreatedDate	:	DateTime
  ModifiedDate	:	DateTime

});

//Return model
module.exports = restful.model('OfferTypes', offerTypes);
